{
  "name": "nextjs-sanity-ecommerce",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:test": "NODE_ENV=\"test\" && next build",
    "start": "next start",
    "start:test": "NODE_ENV=\"test\" && next start",
    "test:ci": "vitest run",
    "test:watch": "vitest watch --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress:start": "start-server-and-test 'yarn start:test' 3000 'yarn cypress:open'",
    "cypress:build": "yarn build:test && yarn cypress:start",
    "cypress:run:chrome": "start-server-and-test 'yarn start:test' 3000 'env-cmd -f .env.test.local cypress run --browser chrome'",
    "cypress:run:firefox": "start-server-and-test 'yarn start:test' 3000 'env-cmd -f .env.test.local cypress run --browser firefox'",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint --ext js,jsx,ts,tsx --fix --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|json|md)\" --write",
    "stylelint": "stylelint **/*.{css,scss,tsx,js} --fix",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@sanity/image-url": "^1.0.2",
    "@stripe/stripe-js": "^2.2.2",
    "@vercel/nft": "^0.26.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.303.0",
    "next": "14.0.4",
    "next-sanity": "^7.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^14.10.0",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "use-shopping-cart": "^3.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/nextjs": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-vite": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "20.10.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.1",
    "@vitest/ui": "1.1.1",
    "autoprefixer": "^10.4.16",
    "commitizen": "^4.3.0",
    "cypress": "^13.6.2",
    "cypress-vite": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.10",
    "msw-storybook-addon": "^1.10.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "prettier-stylelint": "^0.4.2",
    "prisma": "^5.7.1",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.6",
    "stylelint": "^16.1.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.0",
    "typescript": "5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
